    "def rotLeft(a, d):\n",
    "    newA = list(a)\n",
    "    for i in range(d):\n",
    "        value_dropped = newA[0]\n",
    "        newA.pop(0)\n",
    "        newA.append(value_dropped)\n",
    "        #print(newA)\n",
    "    return newA"
